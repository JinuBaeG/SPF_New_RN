{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "typeRoots": ["./@types", "./node_modules/@types"],
    "baseUrl": ".",
    "paths": {
      "@routes/*": ["src/routes/*"],
      "@api/*": ["src/routes/api/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./**/*"]
} 
